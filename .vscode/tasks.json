{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "0.1.0",
    "command": "gulp",
    "isShellCommand": true,
    "tasks": 
    [
        {
            "taskName": "build",
            "isBuildCommand": true,
            "showOutput": "silent",
            "problemMatcher": [
                "$gulp-tsc",
                "$tsc",
                "$tsc-watch"
            ]
        }
    ]
}